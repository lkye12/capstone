n-items: center;\n    flex-direction: column;\n    padding: 0 0;\n  }\n\n  nav > ul > li {\n    padding: 0.5rem;\n    list-style-type: none;\n  }\n\n  #about .wrap {\n    margin-top: 90px;\n  }\n  #about {\n    background: url(\"https://raw.githubusercontent.com/lkye12/capstone/f5781e79e5193c83d42d2979474f484425511240/img/Bio.svg\");\n    background-size: cover;\n  }\n  #about .clear {\n    clear: both;\n  }\n  #about .wrap {\n    background-color: rgb(200, 200, 200);\n    background-color: rgba(200, 200, 200, 0.5);\n    border-radius: 20px;\n    padding: 20px;\n    margin: 40px auto;\n    width: 70%;\n  }\n  #about .container{\n    max-width:1040px;\n    border:1px solid pink;\n    margin:0 auto;\n}\n#about{\n    border:1px solid black;\n    min-height: 500px;\n    background-color:coral\n}\n  #gallery {\n    display: flex;\n    flex-direction: column;\n    align-items: center;\n  }\n}\n/*body*/\nbody {\n  margin: 0%;\n  box-sizing: border-box;\n  overflow-x: hidden;\n  background-color: gray;\n  text-decoration-color: coral;\n}\n/*end body*/\n\n@media (min-width: 550px) {\n  #nav {\n    background: blue;\n    padding: 0 2rem;\n    height: 0rem;\n    min-height: 10vh;\n    overflow: hidden;\n    transition: height 1s ease-in-out;\n  }\n\n  #nav .nav-menu {\n    justify-content: space-between;\n  }\n\n  #nav .toggle-collapse {\n    position: absolute;\n    top: 0%;\n    width: 90%;\n    cursor: pointer;\n    display: none;\n  }\n\n  #nav .toggle-collapse .toggle-icons {\n    display: flex;\n    justify-content: flex-end;\n    padding: 1.7rem 0;\n  }\n\n  #nav .toggle-collapse .toggle-icons i {\n    font-size: 1.4rem;\n    color: var(--text-black);\n  }\n\n  .collapse {\n    height: 30rem;\n  }\n\n  #nav .nav-bar {\n    display: flex;\n    margin: 0;\n  }\n\n  #nav .nav-bar li {\n    padding: 1.6rem 1rem;\n    font-size: 1.1rem;\n    position: relative;\n    font-family: var(--Abel);\n    font-size: 1.1rem;\n  }\n\n  #nav .nav-bar li:hover {\n    background-color: var(--midnight);\n  }\n\n  #nav .nav-bar li:hover a {\n\n  }\n\n  #nav .brand a {\n    font-size: 1.6rem;\n    padding: 1rem 0;\n    display: block;\n    font-family: var(--Lexend);\n    font-size: 1.6rem;\n  }\n\n  /* -------x------- navbar ---------x------- */\n  nav > ul {\n    display: flex;\n    justify-content: space-evenly;\n    padding: 0 0;\n  }\n\n  nav > ul > li {\n    display: flex;\n    text-decoration: none;\n    border: none;\n    padding: 0.5rem 0.5rem;\n    border-radius: 25px;\n    color: var(--white);\n  }\n  nav > ul > li:hover {\n    background-color: var(--midnight);\n\n  }\n\n  nav > .fa-bars {\n    display: none;\n  }\n\n  #bio {\n    margin-right: 3rem;\n  }\n\n\n  #gallery {\n    display: grid;\n    grid-template-columns: repeat(auto-fill, minmax(33%, 1fr));\n    background-color: white;\n  }\n}\n"},"lineCount":238}},"error":null,"hash":"e77f0f4327b92fe35649110274df14e8","cacheData":{"env":{}}}